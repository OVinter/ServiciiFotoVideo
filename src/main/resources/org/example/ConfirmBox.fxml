   <children>
      <Button fx:id="okButton" layoutX="130.0" layoutY="321.0" mnemonicParsing="false" onMousePressed="#setOkButton" prefHeight="27.0" prefWidth="130.0" text="YES" />
      <Button fx:id="noButton" layoutX="352.0" layoutY="321.0" mnemonicParsing="false" onMousePressed="#setNoButton" prefHeight="27.0" prefWidth="130.0" text="NO" />
      <Label fx:id="label" alignment="CENTER" layoutX="65.0" layoutY="68.0" prefHeight="178.0" prefWidth="488.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
